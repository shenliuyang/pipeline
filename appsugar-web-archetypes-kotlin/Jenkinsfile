pipeline {
    agent any
    tools {
        jdk 'jdk-1.8' 
    }
    stages {
        stage('Preparation') {
            steps {
                git  url: 'https://github.com/shenliuyang/appsugar-web-archetypes-kotlin.git'
            }
        }
        stage('Build') {
            steps {
                sh 'git checkout master'
                sh './gradlew  build -x test '
            }
        }
    }
}